<plugin
   id="org.eclipse.emf.test.build"
   name="%pluginName"
   version="2.0.2"
   provider-name="%providerName"
   class="org.eclipse.emf.test.build.EMFTestBuildPlugin">

   <requires>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.junit"/>
      <import plugin="org.eclipse.emf.test.core"/>
      <import plugin="org.eclipse.emf.test.edit"/>
      <import plugin="org.eclipse.emf.test.sdo"/>
      <import plugin="org.eclipse.emf.test.xml"/>
   </requires>

   <runtime>
      <library name="test.build.jar">
         <export name="*"/>
      </library>
   </runtime>
   
</plugin>
