<plugin
   id="org.eclipse.emf.test.performance"
   name="%pluginName"
   version="2.1.0"
   provider-name="%providerName"
   class="org.eclipse.emf.test.performance.EMFTestPerformancePlugin">

   <requires>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.emf.ecore"/>
      <import plugin="org.eclipse.emf.ecore.change"/>
      <import plugin="org.eclipse.emf.ecore.xmi"/>
      <import plugin="org.eclipse.xsd"/>
      <import plugin="org.junit"/>
      <import plugin="org.eclipse.test.performance"/>
      <import plugin="org.eclipse.emf.ecore.sdo"/>
      <import plugin="org.apache.derby" optional="true"/>
   </requires>

   <runtime>
      <library name="test.performance.jar">
         <export name="*"/>
      </library>
   </runtime>
   
</plugin>
