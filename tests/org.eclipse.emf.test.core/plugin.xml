<plugin
   id="org.eclipse.emf.test.core"
   name="Test for EMF Core functions"
   version="2.0.0"
   provider-name="Eclipse.org"
   class="org.eclipse.emf.test.core.EMFTestCorePlugin">

   <runtime>
      <library name="test.core.jar">
         <export name="*"/>
         <packages prefixes="org.eclipse.emf.test.core"/>
      </library>
   </runtime>
   
   <requires>
      <import plugin="org.eclipse.emf.ecore"/>
      <import plugin="org.junit"/>
   </requires>
</plugin>
