<plugin
    name = "%pluginName"
    id = "org.eclipse.emf.java.edit"
    version = "2.1.0"
    provider-name = "%providerName" 
    class = "org.eclipse.emf.java.provider.JavaEditPlugin$Implementation">

  <requires>
    <import plugin="org.eclipse.core.runtime"/>
    <import plugin="org.eclipse.emf.edit" export="true"/>
    <import plugin="org.eclipse.emf.java" export="true"/>
  </requires>

  <runtime>
    <library name="runtime/java.edit.jar">
      <export name="*"/>
    </library>
  </runtime>
</plugin>

